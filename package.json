{
  "name": "morpho-data-structures",
  "version": "1.0.0",
  "description": "Data structures for Morpho's matching engine",
  "repository": "https://github.com/morpho-protocol/morpho-data-structures.git",
  "author": "Morpho Labs <security@morpho.xyz>",
  "license": "AGPL-3.0-only",
  "scripts": {
    "test": "npx hardhat test test-ts/*.ts",
    "coverage": "npx hardhat coverage",
    "lint": "yarn run lint:sol && yarn run lint:ts",
    "lint:ts": "eslint . --ext .ts",
    "lint:sol": "solhint 'contracts/**/*.sol'",
    "fix": "yarn fix:sol && yarn fix:ts",
    "fix:ts": "prettier --config .prettierrc.json './**/*.ts' --write",
    "fix:sol": "prettier --write 'src/**/*.sol'",
    "size": "npx hardhat size-contracts",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^2.1.6",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@types/mocha": "^9.1.0",
    "@types/node": "17.0.8",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.4.1",
    "hardhat": "^2.5.0",
    "hardhat-contract-sizer": "^2.6.1",
    "hardhat-gas-reporter": "^1.0.4",
    "husky": "^7.0.1",
    "prettier": "^2.3.2",
    "prettier-plugin-solidity": "^1.0.0-beta.13",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "0.0.5",
    "solidity-coverage": "^0.7.16",
    "ts-node": "10.4.0",
    "typescript": "4.5.2"
  }
}
